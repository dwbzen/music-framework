{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "chordFormulas"
  ],
  "properties": {
    "chordFormulas": {
      "$id": "#/properties/chordFormulas",
      "type": "array",
      "title": "The Chordformulas Schema",
      "items": {
        "$id": "#/properties/chordFormulas/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "Augmented"
        ],
        "properties": {
          "Augmented": {
            "$id": "#/properties/chordFormulas/items/properties/Augmented",
            "type": "object",
            "title": "The Augmented Schema",
            "required": [
              "symbols",
              "groups",
              "formula",
              "size",
              "chordSize",
              "formulaNumber",
              "template"
            ],
            "properties": {
              "symbols": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/symbols",
                "type": "array",
                "title": "The Symbols Schema",
                "items": {
                  "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/symbols/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "aug",
                    "+"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "groups": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/groups",
                "type": "array",
                "title": "The Groups Schema",
                "items": {
                  "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/groups/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "triad"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "formula": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/formula",
                "type": "array",
                "title": "The Formula Schema",
                "items": {
                  "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/formula/items",
                  "type": "integer",
                  "title": "The Items Schema",
                  "default": 0,
                  "examples": [
                    4,
                    4
                  ]
                }
              },
              "size": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/size",
                "type": "integer",
                "title": "The Size Schema",
                "default": 0,
                "examples": [
                  2
                ]
              },
              "chordSize": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/chordSize",
                "type": "integer",
                "title": "The Chordsize Schema",
                "default": 0,
                "examples": [
                  3
                ]
              },
              "formulaNumber": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/formulaNumber",
                "type": "integer",
                "title": "The Formulanumber Schema",
                "default": 0,
                "examples": [
                  273
                ]
              },
              "template": {
                "$id": "#/properties/chordFormulas/items/properties/Augmented/properties/template",
                "type": "string",
                "title": "The Template Schema",
                "default": "",
                "examples": [
                  "C E Ab"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}
